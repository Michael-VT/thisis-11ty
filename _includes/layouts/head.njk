<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="google-site-verification" content="{{ metadata.googlesitevertification }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title or metadata.title }}</title>
    <meta name="description" content="{{ description or metadata.description }}">
<meta content='{{ title or metadata.title }}' property='og:title'/>
<meta content='https://www.fiverr.com/creativitas/design-your-modern-website-using-jekyll' property='og:author'/>
<meta content='https://www.fiverr.com/creativitas' property='og:developer'/>
<meta content='website' property='og:type'/>
<meta content='{{ title or metadata.title }}' property='og:site_name'/>
<meta content='summary_large_image' name='twitter:card'/>
<meta content='https://twitter.com/home' property='twitter:author'/>
<meta content='@hockeycomp' name='twitter:site'/>
<meta content='{{ cover or metadata.cover }}' name='twitter:image'/>
<meta content='{{ description or metadata.description }}' name='twitter:description'/>
<meta content='{{ title or metadata.title }} ' name='twitter:title'/>
<meta content='{{ title or metadata.title }}' name='keywords'/>
<meta content='index, follow' name='robots'/>
<meta name="generator" content="{{ eleventy.generator }}">
<link rel="icon" type="image/x-icon" href="{{ cover or metadata.cover }}" />
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="alternate" href="{{ metadata.jsonfeed.path | url }}" type="application/json" title="{{ metadata.title }}">
  </head>

